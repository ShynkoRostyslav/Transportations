<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>ee.Servlet_1</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>LogIn</servlet-name>
        <servlet-class>ee.LogIn</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>LogIn</servlet-name>
        <url-pattern>/Log_in</url-pattern>
    </servlet-mapping>




    <servlet>
       <servlet-name>Cabinet</servlet-name>
        <servlet-class>ee.Cabinet</servlet-class>
    </servlet>

    <servlet-mapping>

        <servlet-name>Cabinet</servlet-name>
        <url-pattern>/cabinet</url-pattern>

    </servlet-mapping>

    <context-param>
        <param-name>log4j-config-location</param-name>
        <param-value>WEB-INF/classes/log4j2.properties</param-value>
    </context-param>

</web-app>